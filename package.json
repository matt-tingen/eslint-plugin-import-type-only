{
  "name": "eslint-plugin-import-type-only",
  "version": "1.0.1",
  "author": "Matt Tingen <matt.tingen@gmail.com>",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --clean --dts --format esm,cjs",
    "test": "jest",
    "ts": "tsc --noEmit",
    "lint": "eslint --ext .ts,.tsx src",
    "format": "prettier --write src",
    "prepack": "pnpm ts && pnpm lint && pnpm test && pnpm build"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.26.0",
    "@matt-tingen/eslint-config": "^0.9.0",
    "@types/eslint": "^8",
    "@types/node": "^22.13.10",
    "@typescript-eslint/rule-tester": "^8.26.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^8.26.0"
  }
}
